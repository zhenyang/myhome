[alias]
    st = status -s
    ci = commit
    co = checkout
    br = branch
    oneline = log --pretty=oneline --since='2 days ago' --abbrev-commit
    onelog = log -p -1 --abbrev-commit
    glog = log --graph --oneline --abbrev-commit --decorate --pretty='%h %ci %Cgreen%s%Creset <%aN>'
    l = log --graph --oneline --abbrev-commit --decorate
    unstage = reset HEAD --
last = log -1 HEAD
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
    ui = auto
